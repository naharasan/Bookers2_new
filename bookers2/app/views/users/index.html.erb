<%= render partial: 'books/list' , locals: { user: @user , book: @book} %>
<h2>Users</h2>
<table>
 <tr>
  <th>image</th>
  <th>name</th>
  <th></th>
  <th colspan="3"></th>
 </tr>
 <tbody>
  <% @users.each do |user| %>
   <tr>
    <td><%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpg', fallback: "no_image.jpg" %></td>
    <td><%= user.name %></td>
    <td><%= link_to 'show', user_path(user) %></td>
   </tr>
  <% end %>
 </tbody>
</table>